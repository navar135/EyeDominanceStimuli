        <Param name="Begin JS Routine" updates="constant" val="dot_x = (square_x - (pxPerCm * dot2square_dist));&amp;#10;square_x = (pxPerCm * square2center_dist);&amp;#10;loopN += 1;" valType="extendedCode"/>
