    <Routine name="findBlindSpot">
      <CodeComponent name="blindSpotCode">
        <Param name="Before Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Before JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin Experiment" updates="constant" val="dot_dist_list = []&amp;#10;dist2screen = 0&amp;#10;" valType="extendedCode"/>
        <Param name="Begin JS Experiment" updates="constant" val="dot_dist_list = [];&amp;#10;dist2screen = 0;&amp;#10;" valType="extendedCode"/>
        <Param name="Begin JS Routine" updates="constant" val="dot_x = (square_x - (pxPerCm * dot2square_dist));&amp;#10;square_x = (pxPerCm * square2center_dist);&amp;#10;loopN += 1;" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="dot_x =  square_x - (pxPerCm * dot2square_dist)&amp;#10;square_x = pxPerCm * square2center_dist # it is important to have this defined here and not in begin exp where original incorrect pxPerCm will be used&amp;#10;&amp;#10;loopN += 1&amp;#10;&amp;#10;" valType="extendedCode"/>
        <Param name="Code Type" updates="None" val="Both" valType="str"/>
        <Param name="Each Frame" updates="constant" val="if dot_x&gt;screenEdgePx:&amp;#10;    dot_x = dot_x-1&amp;#10;&amp;#10;else:&amp;#10;    continueRoutine = False&amp;#10;    inLoop.finished = True&amp;#10;    midLoop.finished = True" valType="extendedCode"/>
        <Param name="Each JS Frame" updates="constant" val="if ((dot_x &gt; screenEdgePx)) {&amp;#10;    dot_x = (dot_x - 1);&amp;#10;} else {&amp;#10;    continueRoutine = false;&amp;#10;    inLoop.finished = true;&amp;#10;    midLoop.finished = true;&amp;#10;}" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="" valType="extendedCode"/>
